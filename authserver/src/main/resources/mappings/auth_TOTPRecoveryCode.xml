<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings version="3.1" xmlns="https://jakarta.ee/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence/orm https://jakarta.ee/xml/ns/persistence/orm/orm_3_1.xsd">
    <named-query name="TOTPRecoveryCodeRepository.deleteByUserIdAndCode">
        <query>
            delete from TOTPRecoveryCodeEntity
            where userId = :userId
            and code = :code
        </query>
    </named-query>
    <named-query name="TOTPRecoveryCodeRepository.deleteByUserId">
        <query>
            delete from TOTPRecoveryCodeEntity
            where userId = :userId
        </query>
    </named-query>
</entity-mappings>